{
  "name": "preem-hr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:safe": "concurrently --kill-others-on-fail \"npm run dev\" \"npm run type-check:watch\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "build": "next build",
    "build:vercel": "vercel build",
    "build:test": "npm run type-check && npm run build",
    "prebuild": "npm run type-check",
    "predeploy": "npm run build:test",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "prepare": "husky",
    "vercel:pull": "vercel env pull",
    "vercel:dev": "vercel dev",
    "vercel:deploy": "vercel deploy --prebuilt",
    "check": "./scripts/pre-deploy-check.sh",
    "check:quick": "./scripts/quick-build-check.sh"
  },
